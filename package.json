{
  "name": "hack-ed-site",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://ualbertacompeclub.github.io/HackEDWebsiteBetaPage/",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "es6-object-assign": "^1.1.0",
    "es6-promise": "^4.2.5",
    "google-maps-react": "^2.0.2",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "reactstrap": "^8.0.1",
    "semantic-ui-css": "^2.4.0",
    "semantic-ui-react": "^0.82.5"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "addBetaRemote": "git remote add beta https://github.com/UAlbertaCompEClub/HackEDWebsiteBetaRemote.git",
    "addPrimeRemote": "git remote add prime https://github.com/UAlbertaCompEClub/HackEDWebsitePrimeRemote.git",
    "addRemotes": "npm run addBetaRemote && npm run addPrimeRemote",
    "deploybeta": "gh-pages -d build -o beta",
    "deployprime": "gh-pages -d build -o prime"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.1.1"
  }
}
